<?xml version="1.0"?>
<!-- 
Umbraco examine is an extensible indexer and search engine.
This configuration file can be extended to create your own index sets.
Index/Search providers can be defined in the UmbracoSettings.config

More information and documentation can be found on GitHub: https://github.com/Shazwazza/Examine/
-->
<ExamineLuceneIndexSets>
  <!-- The internal index set used by Umbraco back-office - DO NOT REMOVE -->
  <IndexSet SetName="InternalIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/Internal/"/>

  <!-- The internal index set used by Umbraco back-office for indexing members - DO NOT REMOVE -->
  <IndexSet SetName="InternalMemberIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/InternalMember/">
    <IndexAttributeFields>
      <add Name="id"/>
      <add Name="nodeName"/>
      <add Name="updateDate"/>
      <add Name="writerName"/>
      <add Name="loginName"/>
      <add Name="email"/>
      <add Name="nodeTypeAlias"/>
    </IndexAttributeFields>
  </IndexSet>
    
  <!-- Default Indexset for external searches, this indexes all fields on all types of nodes-->
  <IndexSet SetName="ExternalIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/External/"/>






  <!-- ======================CUSTOM INDEX SETS ======================== -->
  <IndexSet SetName="CompiledPgIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/CompiledPgs/">
    <IndexAttributeFields>
      <add Name="id"/>
      <add Name="nodeName"/>
    </IndexAttributeFields>
    <IndexUserFields>
      <add Name="title"/>
      <add Name="description"/>

      <add Name="heading"/>
      <add Name="content"/>
    </IndexUserFields>
    <IncludeNodeTypes>
      <add Name="compiledPage"/>
      <add Name="segment"/>
    </IncludeNodeTypes>
    <ExcludeNodeTypes/>
  </IndexSet>


  <IndexSet SetName="NavigationIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/Navigation/">
    <IndexAttributeFields>
      <add Name="id"/>
      <add Name="parentID"/>
      <add Name="path"/>
      <add Name="nodeName"/>
      <add Name="urlName"/>
      <add Name="nodeTypeAlias"/>
      <add Name="level"/>
      <add Name="sortOrder" EnableSorting="true"/>
    </IndexAttributeFields>
    <IndexUserFields>
      <add Name="title"/>
      <add Name="showInNavigation"/>
      <add Name="showInFooter"/>
    </IndexUserFields>
    <IncludeNodeTypes>
    </IncludeNodeTypes>
    <ExcludeNodeTypes/>
  </IndexSet>


  <IndexSet SetName="CampaignIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/Campaigns/">
    <IndexAttributeFields>
      <add Name="id"/>
      <add Name="parentID"/>
      <add Name="path"/>
      <add Name="nodeName"/>
      <add Name="urlName"/>
      <add Name="nodeTypeAlias"/>
    </IndexAttributeFields>
    <IndexUserFields>
      <add Name="briefSummary"/>
      <add Name="campaignComplete"/>
      <add Name="completionDate"/>
      <add Name="datePublished"/>
      <add Name="published"/>
    </IndexUserFields>
    <IncludeNodeTypes>
      <add Name="Campaign"/>
    </IncludeNodeTypes>
    <ExcludeNodeTypes/>
  </IndexSet>
  
	
  <!--================================================================-->
  <!--<IndexSet SetName="MediaIndexSet" IndexPath="~/App_Data/TEMP/ExamineIndexes/MediaIndexSet">
    <IndexAttributeFields>
      <add Name="id"/>
      <add Name="nodeName"/>
      <add Name="updateDate"/>
      <add Name="writerName"/>
      <add Name="path"/>
      <add Name="nodeTypeAlias"/>
      <add Name="parentID"/>
    </IndexAttributeFields>
    <IncludeNodeTypes>
      <add Name="File"/>
    </IncludeNodeTypes>
  </IndexSet>-->







</ExamineLuceneIndexSets>